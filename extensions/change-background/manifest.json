{
  "manifest_version": 3,
  "name": "Change Background",
  "version": "1.0",
  "description": "A simple extension to change the background color of the current page.",
  "content_scripts": [
    {
      "matches": [
        "https://gerais.defensoria.mg.def.br/*",
        "https://wp-intranet.defensoria.mg.def.br/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}